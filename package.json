  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npm run clean:dist && ENV=development webpack --config webpack.dev.js --progress --profile --hide-modules --display-optimization-bailout",
    "build:prod": "npm run clean:dist && ENV=production webpack --config webpack.prod.js --color --progress --hide-modules --display-optimization-bailout",
    "clean:dist": "rimraf dist coverage documentation",
    "clean:install": "rimraf node_modules dist coverage && npm install",
    "start:server": "webpack-dev-server --config webpack.dev.js --progress --colors",
    "watch": "npm run start",
    "rimraf": "rimraf"
  },
  "main": "./src/scripts/index.js",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
